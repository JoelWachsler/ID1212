{
  "name": "snake-backend",
  "version": "1.0.0",
  "description": "Backend for the snake project in the Network Programming course ID1221",
  "main": "dist",
  "scripts": {
    "prestart": "yarn run -s build",
    "start": "node dist",
    "dev": "nodemon -w src --exec babel-node src --source-maps --presets env,stage-0",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Joel Wachsler <wachsler@kth.se>",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^4.0.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.13.1",
    "nodemon": "^1.13.0"
  }
}
